<% if !!current_user %>

<%= current_user.inspect %>

<%= react_component 'ContentShow', {content: @content, product_type: @product_type, description: @description, body: @body, image: @content.image_url, update: false, button_text: 'Update Content', current_user: current_user} %>

<% else  %>

<%= react_component 'ContentShow', {content: @content, product_type: @product_type, description: @description, body: @body, image: @content.image_url, update: false, button_text: 'Update Content'} %>

<% end %>
